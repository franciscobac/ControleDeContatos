@{
    ViewData["Title"] = "Editar contato";
}

<div class="text-center">

    <h1 class="display-4">Cadastrar contatos</h1>

</div>

<form>
  <div class="form-group">
    <label for="nome">Nome</label>
    <input type="text" class="form-control" placeholder="Alterar o nome do contato">
  </div>

    <div class="form-group">
        <label for="email">E-mail</label>
        <input type="text" class="form-control" placeholder="Alterar o e-mail do contato">
    </div>

    <div class="form-group">
        <label for="celular">Celular</label>
        <input type="text" class="form-control" placeholder="Alterar o celular do contato">
    </div>

    <br />

    <button type="submit" class="btn btn-primary w-100 d-block">Editar</button>
    <br />
    <a class="btn btn-secondary w-100 d-block" asp-controller="ControllerContato" asp-action="Index" role="button">Voltar</a>
</form>